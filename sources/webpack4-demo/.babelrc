{
    "presets": [["env", {
        "targets": {
            "browsers" : ["last 2 versions", "safari >= 7"]
        }
    }], "react", "stage-1"],
    "plugins": [
        "transform-decorators-legacy",
        "transform-decorators",
        "react-hot-loader/babel",
        ["transform-runtime",
            {
            "helpers": false,
            "polyfill": false,
            "regenerator": true,
            "moduleName": "babel-runtime"
            }
        ]
    ]
}