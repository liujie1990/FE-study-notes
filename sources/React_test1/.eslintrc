{
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "rules": {
        "strict": 0,
        "no-undef": 0,
        "valid-jsdoc": 1,
        "react/jsx-uses-react": 1,
        "react/jsx-uses-vars": 1,
        "react/react-in-jsx-scope": 1,
        "no-class-assign": 0,
        "no-unused-vars": 1,
        "no-console": 1
    },
    "plugins": [
        "react"
    ]
}